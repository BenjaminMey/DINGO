 "method" : 
  {
    "FileIn":
    {
      "inputs":
      {
        "base_directory":   "data_dir",
        "outfields":        ["dti","bval","bvec","mask","regions"]
      }
    },

    "DSI_SRC":
    {
      "connect":
      {
        "source": ["FileIn","dti"],
        "bval": ["FileIn","bval"],
        "bvec": ["FIleIn","bvec"]
      }
    },    
    "DSI_REC":
    {
      "inputs":
      {
            "method": "dti",
            "output_dif":       1,
            "output_tensor":    0,
            "check_btable":     1
      },
      "connect":
      {
            "source":["DSI_SRC","output"],
            "mask":  ["REC_prep","out_file"]
      }
    },
    "DSI_TRK":
